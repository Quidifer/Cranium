{
  "name": "cranium",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prep": "echo '> Installing Node dependencies...' && yarn install && echo '> Installing React dependencies...'  && (cd ./client && yarn install)",
    "dev": "nodemon --watch ./server --watch ./client --ext ts --exec yarn start",
    "dev:client": "yarn build:client && (cd ./client && yarn start)",
    "lint": "eslint .",
    "build": "echo '> Building Server...' && yarn build:typescript && echo '> Building client...'  && yarn build:client",
    "build:typescript": "tsc --project ./server",
    "build:client": "(cd ./client && yarn build)",
    "start": "clear && yarn build && node ./dist/index.js",
    "clean": "yarn clean:typescript && yarn clean:client && yarn db:clean",
    "clean:typescript": "rimraf dist node_modules/.cache",
    "clean:client": "rimraf ./client/build ./client/node_modules/.cache",
    "db:start": "docker-compose up -d",
    "db:fix": "sudo usermod -aG docker $USER && newgrp docker && sudo service docker restart",
    "db:stop": "docker-compose stop",
    "db:clean": "docker-compose down --volumes --remove-orphans"
  },
  "keywords": [],
  "author": "CraniumTeam",
  "license": "ISC",
  "dependencies": {
    "@mikro-orm/core": "^5.6.11",
    "@mikro-orm/migrations": "^5.6.11",
    "@mikro-orm/postgresql": "^5.6.11",
    "@types/mocha": "^10.0.1",
    "@types/uuid": "^9.0.0",
    "create-react-app": "^5.0.1",
    "express": "^4.18.2",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "yarn": "^1.22.19"
  },
  "engines": {
    "node": "v18.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/glob": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4"
  }
}
